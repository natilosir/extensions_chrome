{
  "manifest_version": 3,
  "name": "Natilos.ir",
  "version": "1.0",
  "description": "AI",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://grok.com/*"
      ],
      "css": [
        "grok.css"
      ],
      "js": [
        "grok.js"
      ]
    },
    {
      "matches": [
        "https://chat.deepseek.com/*"
      ],
      "js": [
        "deepseek.js"
      ],
      "css": [
        "deepseek.css"
      ]
    },
    {
      "matches": [
        "https://monica.im/*"
      ],
      "js": [
        "monica.js"
      ],
      "css": [
        "monica.css"
      ]
    },
    {
      "matches": [
        "https://chatgpt.com/*"
      ],
      "js": [
        "chatgpt.js"
      ],
      "css": [
        "chatgpt.css"
      ]
    },
    {
      "matches": [
        "https://chat.qwen.ai/*"
      ],
      "css": [
        "qwen.css"
      ],
      "js": [
        "qwen.js"
      ]
    },
    {
      "matches": [
        "https://food.gau.ac.ir/*"
      ],
      "css": [
        "gau.css"
      ]
    },
    {
      "matches": [
        "https://www.google.com/search*"
      ],
      "css": [
        "google.css"
      ]
    },
    {
      "matches": [
        "https://fa.wikipedia.org/*"
        //        "https://www.google.com/maps*"
      ],
      "css": [
        "wikipedia.css"
      ]
    }
  ]
}

  
